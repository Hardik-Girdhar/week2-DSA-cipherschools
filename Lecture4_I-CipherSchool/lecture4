#include<iostream>
using namespace std;

int main(){

    int a,b,c,d;

    cout<<"Input 4 integers  a,b,c and d:";
    cout<<endl;
    cin>>a>>b>>c>>d;
   

    // execute AND and OR statements and print out the Boolean: 
    
    //for AND condition:
    cout<<(a>b && c>d);
    
    cout<<endl;
    
    //for OR condition:
    cout<<(a>b || c>d);
    
    cout<<endl;

    return 0;
}
